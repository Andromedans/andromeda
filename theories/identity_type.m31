rule Id (⊢ A type) (⊢ a : A) (⊢ b : A) type

rule Id_refl (⊢ A type) (⊢ a : A) : Id A a a

rule Id_ind
  (⊢ A type)
  (x : A, y : A, p : Id A x y ⊢ C type)
  (x : A ⊢ c : C{x, x, (Id_refl A x)})
  (⊢ a : A)
  (⊢ b : A)
  (⊢ p : Id A a b)
  : C{a, b, p}

rule Id_β
  (⊢ A type)
  (x : A, y : A, p : Id A x y ⊢ C type)
  (x : A ⊢ c : C{x, x, (Id_refl A x)})
  (⊢ a : A)
  : Id_ind A C c a a (Id_refl A a) == c{a} : C{a, a, Id_refl A a}
