Parameter unit : Type.
Parameter tt : unit.

Parameter unit_rect: Π [P : unit -> Type] [x : P tt] [u : unit] P u.
Parameter unit_iota: Π [P : unit -> Type] [x : P tt] unit_rect P x tt == x.

Parameter unit_eta : Π [t u : unit] t == u.

Let unit_eq_tt := (λ [x] eta unit_eta in refl x :: x == tt) :: ∀ [x : unit] x == tt.

Inhabit tt.
