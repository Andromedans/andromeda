(* Natural numbers. *)
Parameter nat : Type.

Parameter O : nat.

Parameter S : nat -> nat.

Parameter nat_rect : Π [P : nat -> Type] [s0 : P O] [s : Π [n : nat] P n -> P (S n)] [m : nat] P m.

Parameter nat_iota_O :
  Π [P : nat -> Type] [s0 : P O] [s : Π [n : nat] P n -> P (S n)]
  nat_rect P s0 s O == s0.

Parameter nat_iota_S :
  Π [P : nat -> Type] [s0 : P O] [s : Π [n : nat] P n -> P (S n)] [m : nat]
  nat_rect P s0 s (S m) == s m (nat_rect P s0 s m).
