(** Properties of equality. *)

Let symmetric :=
  (λ [A a b p] beta p in refl b)
  :: Π [A : Type] [a b : A] a ≡ b -> b ≡ a.

Let transitive :=
  (λ [A a b c p q]
     beta p in q)
  :: Π [A : Type] [a b c : A] a ≡ b -> b ≡ c -> a ≡ c.

Let J :=
  (λ [A B r a b p]
    beta p in r b)
  :: Π
       [A : Type]
       [B : Π [a b : A] a ≡ b -> Type]
       [d : Π [x : A] B x x (refl x)]
       [a b : A]
       [p : a ≡ b]
     B a b p.
