(** Properties of equality. *)

(* NB: this technically pollutes the global betas with p_103 : a_101 == b_102 or some such *)
let symmetric =
  (λ A a b p, let _ = beta p in refl b)
  : Π (A : Type) (a b : A), a ≡ b -> b ≡ a

do symmetric

let transitive =
  (λ A a b c p q,
     let _ = beta p in q)
  : Π (A : Type) (a b c : A), a ≡ b -> b ≡ c -> a ≡ c

do transitive

let J =
  (λ A B r a b p,
    let _ = beta p in r b)
  : Π
       (A : Type)
       (B : Π (a b : A), a ≡ b -> Type)
       (d : Π (x : A), B x x (refl x))
       (a b : A)
       (p : a ≡ b),
     B a b p

do J

