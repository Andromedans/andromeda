Parameter N : Type.
Parameter Z : N.
Parameter S : N -> N.
Parameter plus : N -> N -> N.
Parameter plus_Z : forall x : N, plus x Z == x.
Parameter plus_S : forall x y : N, plus x (S y) == S (plus x y).

Check beta plus_S in Type.

Check
  beta plus_Z in
    (refl (S (S Z))) :: plus (S (S Z)) Z == S (S Z).

Check
  beta plus_Z in
  beta plus_S in
   (refl (S (S Z))) :: plus (S Z) (S Z) == S (S Z).