(* Trivial example of using inahbit hints. *)
Parameter A : Type.
Parameter a : A.
Check
  (inhabit a in [] :: [[A]]).

(* Universally quantified inhabit hints. *)
Parameter B : A -> Type.
Parameter f : Π [x : A] B x.

Check
  (inhabit a in
   inhabit f in
    [] :: [[B a]]).

Check inhabit f in (λ [x : A] [] :: [[B x]]) :: Π [x : A] [[B x]].
