constant A : Type
constant B : Type
constant f : B → Type
constant P : ∏ (x : A), f x → Type
check with equality_in betas hints handle
  assume e : A ≡ B in
  let _ = beta e in
  assume x : A in
  assume Y : Type in
  assume h : Y ≡ P (f x) in

  assume X : Type in
  assume h : X ≡ f x in
  let _ = hint h in
  assume g : X → Type in
  assume z : g y in
    z

