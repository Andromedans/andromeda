(* Testing checking for lambdas in various ways. *)

Parameter A : Type.
Parameter B : A -> Type.
Parameter C : Π [x : A] B x -> Type.
Parameter f : Π [x : A] B x.
Parameter g : Π [x : A] [y : B x] C x y.

Parameter a : A.

Check
  (λ [y : B a] g a y) :: Π [y : B a] C a y.

Check (λ [x y] g x y) :: Π [x : A] [y : B x] C x y.

Check (λ [x : A] [y] g x y) :: Π [x : A] [y : B x] C x y.

Check (λ [x] [y : B x] g x y) :: Π [x : A] [y : B x] C x y.
