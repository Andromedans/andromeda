(* Testing checking for lambdas in various ways. *)

constant A : Type
constant B : A -> Type
constant C : Π (x : A), B x -> Type
constant f : Π (x : A), B x
constant g : Π (x : A) (y : B x), C x y

constant a : A

check
  (λ (y : B a), g a y) : Π (y : B a), C a y

check (λ (x y), g x y) : Π (x : A) (y : B x), C x y

check (λ (x : A) (y), g x y) : Π (x : A) (y : B x), C x y (* TODO *)

check (λ (x) (y : B x), g x y) : Π (x : A) (y : B x), C x y

