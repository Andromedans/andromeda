let eqext =
   (lambda A a b p q,
      refl p)
   : forall (A : Type) (a b : A) (p q : a == b), p == q

do eqext

let funext =
  (lambda A B f g p,
    now hints = add_hint p hints in
    refl f : f == g)
  : forall (A : Type) (B : A -> Type) (f g : forall (x : A), B x),
     (forall (x : A), f x == g x) -> f == g

do funext

