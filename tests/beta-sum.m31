let _ = add_beta sum_iota_l
let _ = add_beta sum_iota_r

check
  λ (A B : Type)
          (P : sum A B -> Type)
          (l : ∀ (a : A), P (inl A B a))
          (r : ∀ (b : B), P (inr A B b))
          (b : B),
        refl (r b) : r b == sum_rect A B P l r (inr A B b)

