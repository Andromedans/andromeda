#include "../examples/sum.m31"

let betas = add_beta betas hints sum_iota_l
let betas = add_beta betas hints sum_iota_r

check with equality_in betas hints handle
  λ (A B : Type)
          (P : sum A B -> Type)
          (l : ∀ (a : A), P (inl A B a))
          (r : ∀ (b : B), P (inr A B b))
          (b : B),
        refl (r b) : r b == sum_rect A B P l r (inr A B b)

