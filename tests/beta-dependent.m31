(* Test dependent types in beta hints. *)

Parameter A : Type.
Parameter B : A ⟶ Type.
Parameter C : Π (x : A) ⟶ B x ⟶ Type.

Parameter f : Π (x : A) (y : B x) ⟶ C x y ⟶ B x.
Parameter f_beta : Π (x : A) (y : B x) (z : C x y) ⟶ f x y z == y.

Parameter a : A.
Parameter b : B a.
Parameter c : C a b.

Check
  beta f_beta in
      refl b :: b == f a b c.
