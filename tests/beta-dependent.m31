(* Test dependent types in beta hints. *)

constant A : Type
constant B : A ⟶ Type
constant C : Π (x : A), B x ⟶ Type

constant f : Π (x : A) (y : B x), C x y ⟶ B x
constant f_beta : Π (x : A) (y : B x) (z : C x y), f x y z == y

constant a : A
constant b : B a
constant c : C a b

check
  with equality_in betas hints handle
  let _ = beta f_beta in
      refl b : b == f a b c

