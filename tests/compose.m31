let id =
  λ (A : Type), λ (x : A), x
check id

let compose =
  λ (A B C : Type) (g : B -> C) (f : A -> B) (x : A), g (f x)
check compose


constant X : Type
constant Y : Type
constant Z : Type
constant W : Type
constant x : X
constant f : X -> Y
constant g : Y -> Z
constant h : Z -> W

check refl (g (f x)) : g (f x) == compose X Y Z g f x

check refl (h (g (f x))) :
        compose X Z W h (compose X Y Z g f) x ==
        compose X Y W (compose Y Z W h g) f x

